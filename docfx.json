{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "api/**.yml", "api/index.md" ] },
      { "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ], "exclude": [ "obj/**", "_site/**" ] },
      { "files": [ "tutorials/**.md", "tutorials/**/toc.yml" ] }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "NetOffice Framework",
      "_disableContribution": true,
      "_appFooter": "Copyright Â© 2016 NetOffice Framework.<br>Documentation licensed under <a href=\"https://creativecommons.org/licenses/by/4.0/\">CC BY 4.0<a/>."
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "doc_template"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}